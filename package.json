{
  "name": "create_readme",
  "version": "0.0.7-6",
  "description": "Script and templates to help you Create a README for a new or current project.",
  "homepage": "https://github.com/DavitTec/create_readme#readme",
  "main": "index.js",
  "settings": {
    "CASE_SENSITIVE": true,
    "LOC": "UK",
    "LANG": "en-GB",
    "EOL": "LF"
  },
  "scripts": {
    "todo": "bash ./scripts/update_TODO.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:zenity": "bash ./scripts/test_zenity.sh",
    "release": "standard-version",
    "prelease": "standard-version -p ",
    "deploy": "bash ./scripts/INSERT"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavitTec/create_readme.git"
  },
  "keywords": [
    "readme",
    "developemnt",
    "coding",
    "node"
  ],
  "author": "Davit Mullins (x/@_davit github@DavitTec)",
  "email": "git_create_readem@davit.ie",
  "license": "https://github.com/DavitTec/create_readme/blob/master/LICENSE",
  "bugs": {
    "url": "https://github.com/DavitTec/create_readme/issues"
  },
  "packageManager": "pnpm@10.6.5",
  "devDependencies": {
    "standard-version": "^9.5.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      ""
    ]
  }
}
